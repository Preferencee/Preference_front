<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation2"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.preference.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_mypage"
        android:name="com.example.preference.ui.mypage.MypageFragment"
        android:label="@string/title_mypage"
        tools:layout="@layout/fragment_mypage" />

    <fragment
        android:id="@+id/navigation_test"
        android:name="com.example.preference.ui.test.TestFragment"
        android:label="@string/title_test"
        tools:layout="@layout/fragment_test" />
    <fragment
        android:id="@+id/navigation_community"
        android:name="com.example.preference.ui.community.CommunityFragment"
        android:label="@string/title_communnity" />
